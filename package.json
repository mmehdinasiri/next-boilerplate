{
  "name": "next-boilerplate",
  "private": true,
  "version": "0.0.1",
  "description": "Enterprise-ready Next.js boilerplate with TypeScript, Next.js, Tailwindcss and Scss",
  "author": "Sina Khodabandehloo <khodabandehloo.sina@gmail.com>",
  "homepage": "https://github.com/Sinakhx/next-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/Sinakhx/next-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sinakhx/next-boilerplate.git"
  },
  "license": "MIT",
  "keywords": [
    "next",
    "boilerplate",
    "typescript",
    "react",
    "scss"
  ],
  "scripts": {
    "init:husky": "npx husky-init && husky install && husky set .husky/pre-commit 'npm run typecheck'",
    "dev": "next dev",
    "start": "next start",
    "build": "tsc && next build",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "export": "next export",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:styles": "stylelint --allow-empty-input \"**/*.scss\"",
    "lint:fix": "npm run lint:fix-ts && npm run lint:fix-styles",
    "lint:fix-ts": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:fix-styles": "stylelint --allow-empty-input \"**/*.scss\" --fix",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"**/*.{ts,tsx,scss}\"",
    "typecheck": "tsc --noEmit",
    "docker:img": "docker build -t sample:react-app",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up",
    "push": "git push --follow-tags",
    "release": "standard-version",
    "release:first": "npm run release -- --first-release && npm run push",
    "release:major": "npm run release -- --release-as major && npm run push",
    "release:minor": "npm run release -- --release-as minor && npm run push",
    "release:notag": "npm run release -- --no-git-tag-version && npm run push",
    "release:patch": "npm run release -- --release-as patch && npm run push"
  },
  "dependencies": {
    "@loadable/component": "^5.15.2",
    "@tanstack/react-query": "^4.0.10",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "next": "12.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^6.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@cypress/react": "^6.0.0",
    "@next/bundle-analyzer": "^12.2.4",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "18.6.4",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "cypress": "^10.4.0",
    "eslint": "8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "12.2.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "react-icons": "^4.4.0",
    "sass": "^1.54.3",
    "standard-version": "^9.5.0",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "tailwindcss": "^3.1.8",
    "tailwindcss-labeled-groups": "^0.0.2",
    "typescript": "4.7.4"
  }
}
